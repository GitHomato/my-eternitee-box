submit\" className=\"btn btn-primary\" onClick={handleSubmit}>Create mission !</button>\n\n\n            {submitting && <div>Validating mission...</div>}\n\n\n            <table>\n              <thead>\n                <tr>\n                  <th>Mission Name</th>\n                  <th>Hours</th>\n                  <th>Volunteer Name</th>\n                  <th>Volunteer Address</th>\n                  <th></th>\n                </tr>\n              </thead>\n              <tbody>\n                {missions.map((mission, index) => (\n                  <tr key={index}>\n                    <td>{mission.name}</td>\n                    <td>{mission.hours}</td>\n                    <td>{mission.address}</td>\n                    <td>{mission.id}</td>\n                    <td>\n                    </td>\n                  </tr>\n                ))}\n              </tbody>\n            </table>\n            \n        </form>\n    </div>\n     \n\n    )\n}","/Users/tomshnaider/Desktop/my-eternitee-box/client/src/components/ValidateMission/index.jsx",["57","58","59","60","61","62","63"],"/Users/tomshnaider/Desktop/my-eternitee-box/client/src/components/EterniteePortal/index.jsx",["64","65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":5,"column":16,"nodeType":"72","messageId":"73","endLine":5,"endColumn":24},{"ruleId":"70","severity":1,"message":"74","line":5,"column":26,"nodeType":"72","messageId":"73","endLine":5,"endColumn":35},{"ruleId":"70","severity":1,"message":"75","line":11,"column":12,"nodeType":"72","messageId":"73","endLine":11,"endColumn":23},{"ruleId":"70","severity":1,"message":"76","line":12,"column":12,"nodeType":"72","messageId":"73","endLine":12,"endColumn":24},{"ruleId":"70","severity":1,"message":"77","line":13,"column":12,"nodeType":"72","messageId":"73","endLine":13,"endColumn":26},{"ruleId":"70","severity":1,"message":"78","line":14,"column":12,"nodeType":"72","messageId":"73",